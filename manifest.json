{
  "manifest_version": 3,
  "name": "History Tracker",
  "version": "1.0",
  "description": "Track websites you visit",
  "permissions": [
    "history",
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
